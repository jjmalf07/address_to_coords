<h1>Coordinates Lookup</h1>

<form>
    <div>
      <label for="coords" >Enter Coordinates</label>
      <input name="coordinates" type="text" id="coords">
    </div>

    <div>
      <input type="submit" value="Search">
    </div>
</form>

<dl>
  <dt>Address:</dt>
  <dd><%= @address %></dd>

  <dt>URL Safe Address:</dt>
  <dd><%= @url_safe_address %></dd>

  <dt>Latitude:</dt>
  <dd><%= @latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @longitude %></dd>
</dl>
